{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2019",
    "sourceMap": true,
    "outDir": "dist"
  },
  "exclude": [
    "node_modules"
  ],
  "typedocOptions": {
    "mode": "modules",
    "out": "docs",
    "excludeNotExported": true,
    "excludePrivate": true,
    "excludeProtected": true,
    "theme": "minimal",
    "includeVersion": true,
    "readme": "README.md"
  }
}