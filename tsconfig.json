{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2019",
    "sourceMap": true,
    "outDir": "dist"
  },
  "exclude": [
    "node_modules"
  ],
  "typedocOptions": {
    "inputFiles": [
      "./src"
    ],
    "mode": "modules",
    "out": "docs",
    "excludeNotExported": true,
    "excludePrivate": true,
    "excludeProtected": true,
    "excludeExternals": true,
    "stripInternal": true,
    "includeVersion": true,
    "readme": "README.md",
    "hideGenerator": true,
    "exclude": [
      "**/index.ts",
      "src/opentracing-impl/FoxxTracer.ts",
      "src/helpers/SpanData.ts"
    ]
  }
}